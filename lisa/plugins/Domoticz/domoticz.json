{
    "lang": ["fr", "en"],
    "name": "domoticz",
    "author": "jfcjfc",
    "enabled": 1,
    "pypi-url": "https://pypi.python.org/pypi/lisa-plugin-Domoticz",
    "github-url": "https://raw.githubusercontent.com/Seraf/LISA-PLUGINS-Domoticz/master/lisa/plugins/Domoticz/domoticz.json",
    "version": "0.1.1",
    "description": [
        {
            "lang": "fr",
            "description": "Ce plugin est celui par défaut intégrant les interactions basiques avec LISA",
            "wit-instance": "https://wit.ai/Seraf/lisa_fr"
